{
  "name": "@responsive-image/solid",
  "version": "0.0.0",
  "description": "Solid.js component to render responsive images, provided as locally processed images or loaded remotely from Image CDNs",
  "license": "MIT",
  "author": "Simon Ihmig <simon.ihmig@gmail.com>",
  "repository": "https://github.com/simonihmig/responsive-image",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "concurrently 'pnpm:test:*(!watch)'",
    "test:client": "vitest",
    "test:server": "pnpm run test:client --mode ssr",
    "test:watch": "vitest watch",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:js": "eslint . --cache",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@responsive-image/core": "workspace:^",
    "blurhash": "^2.0.0"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "@vitest/browser": "2.1.6",
    "concurrently": "9.1.0",
    "esbuild": "0.24.0",
    "esbuild-plugin-solid": "0.6.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "playwright": "1.49.1",
    "prettier": "3.4.2",
    "solid-js": "1.9.3",
    "tsup": "8.3.5",
    "tsup-preset-solid": "2.2.0",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vite-plugin-solid": "2.11.0",
    "vitest": "2.1.6"
  },
  "keywords": [
    "solid"
  ]
}
