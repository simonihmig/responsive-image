{
  "name": "@responsive-image/webpack",
  "version": "1.0.0-beta.0",
  "description": "Wepack image loaders, that process and resize images for use with the responsive-image suite.",
  "repository": "https://github.com/simonihmig/ember-responsive-image",
  "license": "MIT",
  "author": "Simon Ihmig <simon.ihmig@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "rollup --config --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepack": "pnpm turbo build"
  },
  "dependencies": {
    "@responsive-image/core": "workspace:^",
    "base-n": "^3.0.0",
    "loader-utils": "^3.2.0",
    "sharp": "^0.33.0"
  },
  "peerDependencies": {
    "@responsive-image/blurhash": "workspace:^",
    "webpack": "^5.70.0"
  },
  "peerDependenciesMeta": {
    "@responsive-image/blurhash": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/preset-typescript": "7.24.1",
    "@tsconfig/ember": "3.0.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@types/sharp": "0.32.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "babel-jest": "29.7.0",
    "concurrently": "8.2.2",
    "css-loader": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "glob": "8.1.0",
    "jest": "29.7.0",
    "memfs": "4.9.2",
    "prettier": "3.2.5",
    "rollup": "4.17.2",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-node-externals": "6.1.2",
    "rollup-plugin-ts": "3.4.5",
    "style-loader": "4.0.0",
    "typescript": "5.4.5",
    "webpack": "5.91.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
