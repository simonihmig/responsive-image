{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": ["src/**/*", "unpublished-development-types/**/*"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true
  }
}
