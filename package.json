    "ember-style-modifier": "^1.0.0",
    "fs-extra": "^10.0.0",
    "minimatch": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sharp": "^0.31.0",
    "silent-error": "^1.1.1",
    "walk-sync": "^3.0.0"
