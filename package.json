    "ember-style-modifier": "^0.6.0",
    "fs-extra": "^10.0.0",
    "minimatch": "^3.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "sharp": "^0.29.0",
    "silent-error": "^1.1.1",
    "walk-sync": "^3.0.0"
