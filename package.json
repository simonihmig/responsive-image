{
  "name": "ember-responsive-image",
  "version": "1.0.0-rc.5",
  "description": "An ember addon that generates resized images and use them in img-tags with the srcset-attribute.",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "test:fastboot": "ember fastboot:test"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "dependencies": {
    "async-q": "^0.3.1",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-stew": "^2.0.1",
    "ember-cli-babel": "^7.1.2",
    "fs-extra": "^7.0.1",
    "sharp": "^0.21.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "broccoli-asset-rev": "^2.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^6.0.0",
    "ember-cli": "~3.6.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-fastboot": "^2.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fastboot-addon-tests": "^0.5.1",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-mocha": "^0.14.0",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.6.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.1.0",
    "eslint-plugin-node": "^8.0.0",
    "jpegquality": "^0.1.7",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "request": "^2.83.0",
    "rsvp": "^4.7.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/kaliber5/ember-responsive-image",
  "license": "MIT",
  "author": "Andreas Schacht <schacht@kaliber5.de>, Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "broccoli-asset-rev"
    ]
  }
}
